<http://w3id.org/rsp/spin/template#user_defined_8>
        a             spin:SelectTemplate ;
        rdfs:comment  "User specified query" ;
        spin:body     [ a                    sp:Select ;
                        sp:resultVariables   ( [ sp:varName  "sensor"^^xsd:string ] [ sp:varName
                                          "ob1"^^xsd:string ] ) ;
                        sp:where             ( [ a                   rsp:NamedWindow ;
                                                 sp:elements         ( [ sp:object     sens-obs:System_C1190 ;
                                                                         sp:predicate  om-owl:procedure ;
                                                                         sp:subject    [ sp:varName  "ob"^^xsd:string ]
                                                                       ] [ sp:object     weather:AirTemperature ;
                                                                           sp:predicate  om-owl:observedProperty ;
                                                                           sp:subject    [ sp:varName  "ob"^^xsd:string ]
                                                                         ] [ sp:object     [ sp:varName  "?0"^^xsd:string ] ;
                                                                             sp:predicate  om-owl:result ;
                                                                             sp:subject    [ sp:varName  "ob"^^xsd:string ]
                                                                           ] [ sp:object     [ sp:varName  "value"^^xsd:string ] ;
                                                                               sp:predicate  om-owl:floatValue ;
                                                                               sp:subject    [ sp:varName  "?0"^^xsd:string ]
                                                                             ] [ sp:object     [ sp:varName  "sensor"^^xsd:string ] ;
                                                                                 sp:predicate  om-owl:procedure ;
                                                                                 sp:subject    [ sp:varName  "ob1"^^xsd:string ]
                                                                               ] [ sp:object     weather:AirTemperature ;
                                                                                   sp:predicate  om-owl:observedProperty ;
                                                                                   sp:subject    [ sp:varName  "ob1"^^xsd:string ]
                                                                                 ] [ sp:object     [ sp:varName  "?1"^^xsd:string ] ;
                                                                                     sp:predicate  om-owl:result ;
                                                                                     sp:subject    [ sp:varName  "ob1"^^xsd:string ]
                                                                                   ] [ sp:object     [ sp:varName  "value1"^^xsd:string ] ;
                                                                                       sp:predicate  om-owl:floatValue ;
                                                                                       sp:subject    [ sp:varName  "?1"^^xsd:string ]
                                                                                     ] ) ;
                                                 rsp:windowNameNode  :w
                                               ] [ a              sp:Filter ;
                                                   sp:expression  [ a        sp:gt ;
                                                                    sp:arg1  [ sp:varName  "value1"^^xsd:string ] ;
                                                                    sp:arg2  [ sp:varName  "value"^^xsd:string ]
                                                                  ]
                                                 ] ) ;
                        rsp:fromNamedWindow  [ a                rsp:LogicalWindow ;
                                               rsp:logicalStep  "PT5S"^^xsd:duration ;
                                               rsp:range        "PT5S"^^xsd:duration ;
                                               rsp:streamIri    :s ;
                                               rsp:windowIri    :w
                                             ] ;
                        rsp:registerAs       :q7
                      ] .
