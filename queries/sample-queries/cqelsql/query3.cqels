PREFIX  :     <http://debs2015.org/streams/>
PREFIX  debs: <http://debs2015.org/pred#>

CONSTRUCT 
  { 
    ?location debs:profit ?totalamount .
  }
WHERE
  { { SELECT  (SUM(?amount) AS ?totalamount) ?location
      WHERE
        {  }
      GROUP BY ?location
      ORDER BY DESC(?totalamount)
      LIMIT   3
    }
  }
